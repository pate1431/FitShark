<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    android:name="androidx.navigation.fragment.NavHostFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:defaultNavHost="true"
    app:navGraph="@navigation/navigation"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="project.stN991536967.snehal.loginFragment"
        android:label="Login">
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="project.stN991536967.snehal.registerFragment"
        android:label="Register">
        <action
            android:id="@+id/action_registerFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="project.stN991536967.snehal.homeFragment"
        android:label="Home">
        <action
            android:id="@+id/action_homeFragment_to_bmiFragment"
            app:destination="@id/bmiFragment"
            app:popUpTo="@id/bmiFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_homeFragment_to_addlogsFragment"
            app:destination="@id/addlogsFragment"
            app:popUpTo="@id/addlogsFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_homeFragment_to_viewlogFragment"
            app:destination="@id/viewlogFragment"
            app:popUpTo="@id/viewlogFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_homeFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/bmiFragment"
        android:name="project.stN991536967.snehal.bmiFragment"
        android:label="BMI Calculator" />
    <fragment
        android:id="@+id/addlogsFragment"
        android:name="project.stN991536967.snehal.addlogsFragment"
        android:label="Add Exercise">
        <action
            android:id="@+id/action_addlogsFragment_to_viewlogFragment"
            app:destination="@id/viewlogFragment"
            app:popUpTo="@id/viewlogFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/viewlogFragment"
        android:name="project.stN991536967.snehal.viewlogFragment"
        android:label="Exercise">
        <action
            android:id="@+id/action_viewlogFragment_to_addlogsFragment"
            app:destination="@id/addlogsFragment"
            app:popUpTo="@id/addlogsFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_viewlogFragment_to_updateFragment"
            app:destination="@id/updateFragment" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="project.stN991536967.snehal.welcomeFragment"
        android:label="Welcome">
        <action
            android:id="@+id/action_welcomeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_welcomeFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_help"
            app:destination="@id/help" />
        <action
            android:id="@+id/action_welcomeFragment_to_about"
            app:destination="@id/about" />
    </fragment>
    <fragment
        android:id="@+id/updateFragment"
        android:name="project.stN991536967.snehal.updateFragment"
        android:label="updateFragment" >
        <action
            android:id="@+id/action_updateFragment_to_viewlogFragment"
            app:destination="@id/viewlogFragment" />
    </fragment>
    <fragment
        android:id="@+id/about"
        android:name="project.stN991536967.snehal.About"
        android:label="About" >
        <action
            android:id="@+id/action_about_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/help"
        android:name="project.stN991536967.snehal.Help"
        android:label="Help" >
        <action
            android:id="@+id/action_help_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
</navigation>